              "properties":{
                "_isEnabled": {
                  "type":"boolean",
                  "required":true,
                  "default": "true",
                  "title":"Is Enabled",
                  "inputType": {"type": "Boolean", "options": [true, false]},
                  "validators": [],
                  "help": "Enables Audio Extension"
                }
              }
            }
          }
        },
