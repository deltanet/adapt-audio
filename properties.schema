        "contentobject": {
          "type":"object"
        },
        "article": {
          "type":"object",
          "properties":{
            "_audio": {
              "type":"object",
              "required":true,
              "legend": "Audio Extension",
              "properties":{
                "_isEnabled": {
                  "type":"boolean",
                  "required":true,
                  "title": "Enabled",
                  "inputType": { "type": "Boolean", "options": [false, true]},
                  "validators": [],
                  "help": "Turns Audio on and off. Acceptable values are true and false."
                },
                "_showControls": {
                  "type":"boolean",
                  "required":true,
                  "title": "Show Audio Controls",
                  "inputType": { "type": "Boolean", "options": [false, true]},
                  "validators": [],
                  "help": "Turns Audio Countrols on and off. Acceptable values are true and false."
                },
                "_autoplay": {
                  "type":"boolean",
                  "required":true,
                  "title": "Autoplays Audio",
                  "inputType": { "type": "Boolean", "options": [false, true]},
                  "validators": [],
                  "help": "Turns Audio Autoplay on and off. Acceptable values are true and false."
                },
                "_channel": {
                  "type":"number",
                  "required":true,
                  "default": 0,
                  "title": "Channel Number",
                  "inputType": "Number",
                  "validators": ["required", "number"],
                  "help": "channels represent the number of items defined in the course.json settings, by default there are 0, 1 and 2 available (3 _audioItems in the array)"
                },
                "_location": {
                  "type": "string",
                  "required": true,
                  "enum": ["top-left", "top-right", "bottom-left", "bottom-right"],
                  "inputType": {"type": "Select", "options": ["top-left", "top-right", "bottom-left", "bottom-right"]},
                  "title": "Set position for audio controls",
                  "validators": ["required"],
                  "help": "pre-defined styles are top-left, top-right, bottom-left, bottom-right"
                },
                "_media": {
                  "type":"object",
                  "required":true,
                  "properties":{
                    "mobile": {
                      "type":"string",
                      "required":true,
                      "title": "Mobile MP3 Audio Select",
                      "default": "",
                      "inputType": "Asset:audio",
                      "validators": ["required"],
                      "help": "This is the audio source used when page is viewed at mobile resolution"
                    },
                    "desktop": {
                      "type":"string",
                      "required":true,
                      "title": "Desktop MP3 Audio Select",
                      "default": "",
                      "inputType": "Asset:audio",
                      "validators": ["required"],
                      "help": "This is the audio source used when page is viewed at desktop resolution"
                    }
                  }
                }
