                  "inputType": "Text",
                  "validators": [],
                  "help": "Audio Description"
                },
                "_audioItems": {
                  "type": "array",
                  "required": false,
                  "title": "Resources Items",
                  "items": {
                    "type": "object",
                    "required": false,
                    "title": "Items",
