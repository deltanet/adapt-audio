                          "title": "Full",
                          "inputType": "Text",
                          "validators": []
                        },
                        "reduced": {
                          "type": "string",
                          "require": false,
                          "default": "Reduced",
                          "title": "Reduced",
                          "inputType": "Text",
                          "validators": []
                        },
                        "continue": {
                          "type": "string",
                          "require": false,
                          "default": "Continue",
                          "title": "Continue",
                          "inputType": "Text",
                          "validators": []
                        },
                        "turnOff": {
                          "type": "string",
                          "require": false,
                          "default": "Turn audio off",
                          "title": "Turn audio off",
                          "inputType": "Text",
                          "validators": []
                        }
                      }
                    },
                    "_graphic": {
                      "type":"object",
                      "required":true,
                      "title": "Choose your Audio icon image",
                      "properties":{
                        "src": {
                          "type":"string",
                          "required":true,
                          "default": "",
                          "title": "Audio icon Select",
                          "inputType": "Asset:image",
                          "validators": ["required"],
                          "help": "This is the audio icon used"
                        }
                      }
                    }
                  }
                },
                "_togglePrompt": {
                  "type":"object",
                  "required":false,
                  "title": "Prompt",
