                    "bodyNoReduced": {
                      "type": "string",
                      "require": false,
                      "default": "Audio is turned on by default in this module. Please select Continue or Turn audio off (you can change your preference at any time by selecting the <span class='speaker-icon fa-volume-up audio-notify-icon'></span> icon).",
                      "title": "Title",
                      "inputType": "Text",
                      "validators": []
                    },
                    "_buttons": {
                      "type":"object",
                      "required":true,
                      "title": "Button Names",
                      "properties":{
                        "full": {
                          "type": "string",
                          "require": false,
