                      "title": "Body",
                      "inputType": "Text",
                      "validators": []
                    },
                    "titleNoReduced": {
                      "type": "string",
                      "require": false,
                      "default": "Audio setting",
                      "title": "Title",
                      "inputType": "Text",
                      "validators": []
